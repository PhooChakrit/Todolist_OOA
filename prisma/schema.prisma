datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}
model User {
  id Int @id @default(autoincrement())
  email String @unique
  name String?
  picture String?
  todos Todoitem[] @relation("UserEmails")
}
model Todoitem {
  id Int @id @default(autoincrement())
  userEmail String
  title String 
  data String
  status String
  user User @relation(fields: [userEmail], references: [email], name: "UserEmails")
}